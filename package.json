{
  "dependencies": {
    "@types/minimatch": "3.0.3",
    "cheerio": "1.0.0-rc.2",
    "minimatch": "3.0.3",
    "typescript": "4.0.5"
  },
  "scripts": {
    "test": "mocha -r ts-node/register src/**/*_test.ts",
